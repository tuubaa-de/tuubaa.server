{
  "name": "tuubaa.server",
  "description": "Backend for tuubaa Discord and Website",
  "version": "0.2.1",
  "repository": {
    "url": "https://github.com/tuubaa-de/tuubaa.server"
  },
  "author": "Root-Time",
  "license": "MIT",
  "homepage": "tuubaa.de",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "concurrently  \"nodemon src/index.ts\"",
    "build:watch": "tsc-watch --onSuccess \"yalc push\"",
    "db:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:generate": "dotenv -e .env.development -- npx prisma generate",
    "db:pull": "dotenv -e .env.development -- npx prisma db pull",
    "db:push": "dotenv -e .env.development -- npx prisma db push"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@types/node": "^20.12.12",
    "axios": "^1.7.0",
    "canvafy": "^7.1.0",
    "canvas": "^2.11.2",
    "cheerio": "^1.0.0-rc.12",
    "colorthief": "^2.4.0",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "discord.js": "^14.14.1",
    "dotenv": "^16.3.1",
    "ffmpeg-static": "^5.2.0",
    "greetify": "^2.0.1",
    "http": "^0.0.1-security",
    "imagemagick": "^0.1.3",
    "mediaplex": "^0.0.9",
    "moment": "^2.30.1",
    "moonlink.js": "^3.6.8",
    "musicard": "^2.0.5",
    "node-vibrant": "^3.2.1-alpha.1",
    "rankcard": "^1.0.8-test",
    "rc-color": "^1.0.0",
    "youtube-ext": "^1.1.25",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/imagemagick": "^0.0.35",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0",
    "prisma": "^5.13.0",
    "tsc-watch": "^6.2.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  }
}
